Cell["\<\
The theoretical results include the Thompson scattering from the classical \
Electrodynamics and Klein-Nishina formula from the quantum Electrodynamics. \
They are defined here.\
\>", "Text",
 CellChangeTimes->{{3.7316549525011554`*^9, 
  3.731655000532922*^9}},ExpressionUUID->"0fbbc0d4-75bb-4977-88f3-\
22c6e636b80f"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"KleinNishina", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"EngRatio", "[", "\[Theta]", "]"}], "^", "2"}], "*", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"EngRatio", "[", "\[Theta]", "]"}], "+", 
       RowBox[{
        RowBox[{"EngRatio", "[", "\[Theta]", "]"}], "^", 
        RowBox[{"-", "1"}]}], "-", 
       RowBox[{
        RowBox[{"Sin", "[", 
         RowBox[{"\[Theta]", " ", "\[Degree]"}], "]"}], "^", "2"}]}], ")"}], 
     "/", "2"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Thompson", "[", "\[Theta]_", "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{"\[Theta]", " ", "\[Degree]"}], "]"}], "^", "2"}]}], ")"}], 
    "/", "2"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.7316061207705383`*^9, 3.7316062940473013`*^9}, {
  3.7316063926855497`*^9, 
  3.731606394347681*^9}},ExpressionUUID->"e3e08b81-2447-436e-9f23-\
1127e599bd1f"],

